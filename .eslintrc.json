{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": 1,
        "linebreak-style":"off",
        "object-curly-spacing":"off",
        "new-cap":"off",
        "comma-dangle":"off",
        "max-len":"off",
        "valid-jsdoc":"off",
        "require-jsdoc":"off",
        "camelcase":"off"
    }
}
